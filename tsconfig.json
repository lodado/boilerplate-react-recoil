{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "moduleResolution": "node",
      "noResolve": false,
      "noImplicitAny": false,
      "removeComments": false,
      "sourceMap": true,
      "allowJs": true,
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "keyofStringsOnly": true
    },
    "baseUrl": "./",
    "include": ["frontend/javascripts/**/*"],
    "exclude": ["./node_modules/**/*", "**/*.spec.ts"]
   }